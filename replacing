count=1
while [ $count -lt 7 ]
do 
	echo "aws"
	
	if [ $count -eq 5 ]
	then
		break
	fi
		echo $count
		count=`expr $count + 1`

	done

count=1
while [ $count -lt 7 ]
do
        echo "devops"

        if [ $count -eq 5 ]
        then
                break
        fi
                echo $count
                count=`expr $count + 1`

        done

